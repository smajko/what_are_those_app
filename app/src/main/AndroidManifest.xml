<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.turntotech.photocapture"
    android:versionCode="1"
    android:versionName="1.0" >


    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.turntotech.photocapture.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="org.turntotech.photocapture.AgeActivity"
            android:label="title_age_activity" >
        </activity>

        <activity
            android:name="org.turntotech.photocapture.SymptomActivity"
            android:label="title_symptom_activity" >
        </activity>

        <activity
            android:name="org.turntotech.photocapture.CropActivity"
            android:label="title_crop_activity" >
        </activity>
    </application>

</manifest>
